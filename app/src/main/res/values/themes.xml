<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="PracticeApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorApp</item>
        <item name="colorPrimaryVariant">@color/colorApp</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="splashTheme" parent="PracticeApp">
        <item name="android:windowBackground">@drawable/splash_background</item>
    </style>

    <style name="registroTheme" parent="PracticeApp">
        <item name="colorPrimaryVariant">@color/colorApp</item>
        <item name="colorPrimary">#9A9393</item>
    </style>
    <style name="buscar_activity" parent="PracticeApp">
        <item name="colorPrimary">@color/colorApp</item>
        <item name="colorPrimaryVariant">@color/colorApp</item>
        <item name="colorOnPrimary">@color/black</item>


    </style>
</resources>